}
.mini-sidebar.expand-menu .active.subdrop ~ ul {
    @apply block;
}
.active.subdrop ~ ul {
    @apply block;
}
.mini-sidebar .header #toggle_btn {
    @apply opacity-0 h-0 w-0;
}
.mini-sidebar.expand-menu .header #toggle_btn {
    @apply opacity-100 h-auto w-auto;
}

.page-wrapper {
    -webkit-transition: all 0.5s ease;
    -ms-transition: all 0.5s ease;
    transition: all 0.5s ease;
    margin: 0 0 0 252px;
    position: relative;
    left: 0;
    -webkit-transition: all 0.2s ease;
    -ms-transition: all 0.2s ease;
    transition: all 0.2s ease;
}

@media (max-width: screen(lg)) {
    .page-wrapper {
        margin: 0;
        padding: 60px 0 0;
    }
}
.page-wrapper .content {
    @apply p-6 pb-0;

    &:max-lg {
        @apply p-4 pb-0;
    }

    &.content-two {
        @apply pb-6;

        &:max-md {
            @apply pb-4;
        }
    }
}

.page-header {
    @apply flex items-center justify-between flex-wrap mb-2.5;
}

@media (max-width: 575.98px) {
    .page-header {
        @apply flex-col items-start;
    }
}
