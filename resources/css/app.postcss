@import "tailwindcss";
@import "tailwindcss-primeui";
@import "primeicons/primeicons.css";

@custom-variant dark (&:where(.dark, .dark *));

@theme {
    --breakpoint-sm: 36rem; /* Bootstrap sm: 576px */
    --breakpoint-md: 48rem; /* Bootstrap md: 768px */
    --breakpoint-lg: 62rem; /* Bootstrap lg: 992px */
    --breakpoint-xl: 75rem; /* Bootstrap xl: 1200px */
    --breakpoint-2xl: 87.5rem; /* Bootstrap xxl: 1400px */

    --container-3xs: 16rem; /* Custom - Not used in Bootstrap */
    --container-2xs: 18rem; /* Custom */
    --container-xs: 20rem; /* Custom */

    --container-sm: 33.75rem; /* Bootstrap sm - 540px */
    --container-md: 45rem; /* Bootstrap md - 720px */
    --container-lg: 60rem; /* Bootstrap lg - 960px */
    --container-xl: 71.25rem; /* Bootstrap xl - 1140px */
    --container-2xl: 82.5rem; /* Bootstrap xxl - 1320px */

    --container-3xl: 90rem; /* Custom large (1440px) */
    --container-4xl: 96rem; /* Custom */
    --container-5xl: 102.4rem; /* Custom (1638.4px) */
    --container-6xl: 115.2rem; /* Custom (1843.2px) */
    --container-7xl: 128rem; /* Custom (2048px) */
}

/* Light */
:root {
    --p-primary-50: #f2f7f8;
    --p-primary-100: #c2eaf0;
    --p-primary-200: #a6e7f0;
    --p-primary-300: #85e5f1;
    --p-primary-400: #8cbbc2;
    --p-primary-500: #69c1cc;
    --p-primary-600: #4aafbd;
    --p-primary-700: #258794;
    --p-primary-800: #0c5d68;
    --p-primary-900: #0a525c;
    --p-primary-950: #043036;
    --p-surface-0: #ffffff;
    --p-surface-50: #fafafa;
    --p-surface-100: #f4f4f5;
    --p-surface-200: #e4e4e7;
    --p-surface-300: #d4d4d8;
    --p-surface-400: #a1a1aa;
    --p-surface-500: #71717a;
    --p-surface-600: #52525b;
    --p-surface-700: #3f3f46;
    --p-surface-800: #27272a;
    --p-surface-900: #18181b;
    --p-surface-950: #09090b;
    --p-content-border-radius: 6px;
    --p-primary-color: var(--p-primary-500);
    --p-primary-contrast-color: var(--p-surface-0);
    --p-primary-hover-color: var(--p-primary-600);
    --p-primary-active-color: var(--p-primary-700);
    --p-content-border-color: var(--p-surface-200);
    --p-content-hover-background: var(--p-surface-100);
    --p-content-hover-color: var(--p-surface-800);
    --p-highlight-background: var(--p-primary-50);
    --p-highlight-color: var(--p-primary-700);
    --p-highlight-focus-background: var(--p-primary-100);
    --p-highlight-focus-color: var(--p-primary-800);
    --p-text-color: var(--p-surface-700);
    --p-text-hover-color: var(--p-surface-800);
    --p-text-muted-color: var(--p-surface-500);
    --p-text-hover-muted-color: var(--p-surface-600);
    --body-bg-gradient: linear-gradient(
        180deg,
        rgba(245, 254, 253, 0.9) 0%,
        rgba(255, 255, 255, 0.99) 100%
    );
    --body-bg-image: url(/assets/img/bg/patternlight.png);
    --body-bg-image-size: 4.6875rem;
    --body-bg-cover: url(/hud-vue/assets/cover-dhlOIT5m.jpg);
    --body-font-weight: 400;
    --body-letter-spacing: 0.046875rem;
    --app-header-bg: var(--p-surface-0);
    --app-header-brand-img: url(data:image/png;base64,);
    --app-header-link-color: var(--p-surface-900);
    --app-header-link-hover-color: var(--p-surface-950);
    --app-sidebar-menu-header-color: rgba(var(--inverse-rgb), 0.3);
    --app-sidebar-menu-link-color: rgba(var(--inverse-rgb), 0.5);
    --app-sidebar-menu-link-hover-color: --p-surface-950;
    --app-sidebar-menu-link-active-color: --p-surface-950;
    --theme-bg-subtle: #d3ebe4;
}

body {
    @apply w-screen h-[100vh] overflow-x-hidden;
}

html:before {
    content: "";
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    height: 100%;
    z-index: -5;
    background: var(--body-bg-gradient);
}

body:before {
    content: "";
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    height: 100%;
    background-color: transparent;
    background-image: var(--body-bg-image);
    background-size: var(--body-bg-image-size);
    background-repeat: repeat;
    background-attachment: initial;
    z-index: -5;
}

h1 {
    font-size: 2.5rem; /* Bootstrap h1 (40px) */
    font-weight: 700;
}
h2 {
    font-size: 2rem; /* Bootstrap h2 (32px) */
    font-weight: 600;
}
h3 {
    font-size: 1.75rem; /* Bootstrap h3 (28px) */
    font-weight: 600;
}
h4 {
    font-size: 1.5rem; /* Bootstrap h4 (24px) */
    font-weight: 600;
}
h5 {
    font-size: 1.25rem; /* Bootstrap h5 (20px) */
    font-weight: 500;
}
h6 {
    font-size: 1rem; /* Bootstrap h6 (16px) */
    font-weight: 500;
}
p {
    font-size: 1rem; /* Bootstrap paragraph (16px) */
    line-height: 1.6;
    color: rgba(var(--p-highlight-color), 0.5); /* Bootstrap text-dark */
}
strong {
    font-weight: 700;
}
em {
    font-style: italic;
}
blockquote {
    font-size: 1.25rem;
    font-style: italic;
    border-left: 4px solid #dee2e6; /* Bootstrap blockquote border */
    padding-left: 1rem;
    color: #6c757d; /* Bootstrap muted color */
}

.list-group a.active {
    color: rgb(61, 94, 225);
    background: rgb(236, 239, 252);
}

.p-menubar.app-header {
    @apply h-14 bg-[var(--app-header-bg)] transition-all duration-150 ease-in-out border-0 rounded-t-md;
    & .brand {
        height: 100%;
        width: 18.875rem;
        padding: 0 1rem;
        display: flex;
        align-items: center;

        & .brand-logo {
            text-decoration: none;
            color: var(--app-header-link-color);
            font-size: 0.91875rem;
            letter-spacing: 2px;
            display: flex;
            align-items: center;

            & .brand-img {
                width: 1.625rem;
                height: 1.625rem;
                background: var(--app-header-brand-img);
                background-size: cover;
                background-position: center;
                background-repeat: no-repeat;
                display: flex;
                align-items: center;
                justify-content: center;
                margin-right: 1rem;

                & .brand-img-text {
                    display: block;
                    letter-spacing: initial;
                    font-weight: 600;
                    font-size: 1.05rem;
                }
            }
        }
    }
}

@media (min-width: 992px) {
    .mini-sidebar.expand-menu .header-left {
        @apply flex-row justify-between flex px-[15px] py-0;
    }
    .mini-sidebar .header-left {
        @apply flex items-center justify-center w-20 flex-col p-0;
    }
    .mini-sidebar .header-left .logo-small {
        @apply block pt-2.5 pb-0 px-0;
    }
    .mini-sidebar .header-left .logo {
        @apply hidden;
    }
    .mini-sidebar .menu-arrow {
        @apply hidden;
    }
    .mini-sidebar .chat-user {
        @apply hidden;
    }
    .mini-sidebar .user-menu .badge {
        @apply flex;
    }
    .mini-sidebar.expand-menu .menu-arrow {
        @apply block;
    }
    .mini-sidebar.expand-menu .chat-user {
        @apply block;
    }
    .mini-sidebar.expand-menu .logo-small {
        @apply hidden;
    }
    .mini-sidebar.expand-menu .logo {
        @apply block;
    }
    .mini-sidebar.expand-menu #toggle_btn {
        @apply opacity-100;
    }
    .mini-sidebar .noti-dot:before {
        @apply hidden;
    }
}

.sidebar {
    @apply flex bg-[var(--app-header-bg)] z-[1001] fixed transition-all duration-[0.5s] ease-[ease] w-[252px] mt-0 border-r-[--theme-bg-subtle] border-r border-solid left-0 top-14 bottom-0;

    .sidebar-inner {
        @apply w-full;
    }

    .sidebar-menu {
        @apply p-[15px];

        & > ul {
            li .submenu > ul li a {
                @apply text-sm relative pl-10 before:content-[""] before:w-[5px] before:h-[5px] before:absolute before:-translate-y-2/4 before:rounded-[50%] before:left-7 before:top-2/4 before:bg-primary-200;
            }

            & > li {
                @apply mb-6;

                &:last-child ul {
                    @apply mb-0;
                }
                &.active a {
                    @apply text-[--p-text-color] rounded-[5px];
                }
                &.active svg {
                    @apply text-[--p-primary-300];
                }
                svg {
                    @apply min-w-[16px] w-4 h-4 mr-2.5;
                }
                & > a {
                    @apply flex items-center relative text-primary px-[15px] py-2.5;
                }
                &.submenu ul li a {
                    @apply font-medium text-sm relative block pl-[30px] pr-2 py-2;
                }
                ul {
                    /* @apply block mb-[15px]; */
                    li {
                        @apply overflow-hidden mb-[5px] rounded-[5px];
                        a {
                            @apply flex items-center font-normal text-sm dark:text-primary text-color relative w-full p-2 dark:hover:text-[--p-primary-hover-color];
                            &.active span {
                                @apply text-primary-contrast;
                            }
                        }
                    }
                }
            }
        }
    }
}
@media (max-width: 991.98px) {
    .sidebar {
        @apply ml-[-575px] transition-all duration-[0.4s] ease-[ease] z-[1041];
    }
}
@media (max-width: 575.98px) {
    .sidebar {
        @apply w-full;
    }
}
.sidebar .sidebar-menu > ul li .submenu > a.subdrop {
    @apply bg-primary-200;
}
.sidebar .sidebar-menu > ul li .submenu > a.subdrop i {
    @apply text-primary-300 bg-primary-contrast;
}
.sidebar .sidebar-menu > ul li .submenu > a.active {
    @apply rounded-[5px] bg-primary dark:bg-primary-200;
}
.sidebar .sidebar-menu > ul li .submenu > a.active i {
    @apply text-primary-300 bg-primary-contrast;
}
.sidebar .sidebar-menu > ul li .submenu > a.active span {
    @apply text-primary-300;
}
.sidebar .sidebar-menu .menu-arrow {
    @apply flex items-center justify-center text-lg leading-4 size-4 absolute transition-all duration-[0.2s] ease-[ease] rounded-[50%] right-2.5 top-2/4;
}
.sidebar .sidebar-menu.subdrop .menu-arrow {
    @apply -translate-y-2/4 rotate-90 bg-transparent;
}
.sidebar .sidebar-left {
    @apply w-[100px] shadow-[5px_0px_15px_rgba(0,0,0,0.05)];
}
.sidebar .sub-menu a {
    @apply flex items-center justify-between;
}
.sidebar li a.subdrop .menu-arrow {
    @apply -translate-y-2/4 rotate-90;
}
.sidebar .sidemenu a {
    @apply w-full font-medium block mt-0 mb-2.5 mx-0 p-2.5 rounded-[5px] hover:text-primary-contrast;
}
.sidebar .sidemenu a:hover {
    background: linear-gradient(46.62deg, #3d5ee1 0%, #673ab7 93.64%);
}
.sidebar .sidemenu a:hover svg {
    @apply text-color;
}
.sidebar .sidemenu a svg {
    @apply stroke-[1px];
}
@media (min-width: 992px) {
    .mini-sidebar .page-wrapper {
        @apply ml-20;
    }
    .mini-sidebar .user-names {
        @apply hidden;
    }
    .mini-sidebar .header-left #toggle_btn {
        @apply opacity-100 rotate-180 after:border-0;
    }
    .mini-sidebar .sidebar .sidebar-menu .clinicdropdown > a {
        @apply p-[5px];
    }
    .mini-sidebar .sidebar .sidebar-menu > ul > li ul li.submenu ul li a {
        @apply pl-10;
    }
    .mini-sidebar.expand-menu .header-left #toggle_btn {
        @apply opacity-100 rotate-180;
    }
    .mini-sidebar.expand-menu .sidebar .sidebar-menu .clinicdropdown > a {
        @apply px-[15px] py-2.5;
    }
    .mini-sidebar .sidebar-right {
        @apply hidden;
    }
    .mini-sidebar .sidebar .sidebar-menu ul li a span {
        @apply hidden;
    }
    .mini-sidebar .sidebar .sidebar-menu ul li svg {
        @apply mr-0;
    }
    .mini-sidebar .sidebar {
        @apply w-20 overflow-hidden;
    }
    .mini-sidebar.expand-menu .sidebar {
        @apply block;
    }
    .expand-menu.mini-sidebar .sidebar .sidebar-menu .submenu-hdr {
        @apply flex text-xs after:ml-2;
    }
    .expand-menu.mini-sidebar .sidebar .user-names {
        @apply hidden;
    }
    .expand-menu .sidebar-right {
        @apply block transition-all duration-[0.2s] ease-[ease];
        -webkit-transition: all 0.2s ease;
        -ms-transition: all 0.2s ease;
    }
    .expand-menu .sidebar {
        @apply w-[252px];
    }
    .expand-menu .sidebar .sidebar-menu ul > li > a {
        @apply p-2;
    }
    .expand-menu .sidebar .sidebar-menu ul > li > a span {
        @apply inline-block leading-[0];
    }
    .expand-menu .sidebar .sidebar-menu ul > li svg {
        @apply mr-2.5;
    }
    .expand-menu .sidebar .sidebar-menu .submenu-hdr {
        @apply flex;
    }
    .expand-menu .header-left {
        @apply w-[252px];
    }
    #toggle_btn {
        @apply flex items-center justify-center float-left;
        display: -webkit-box;
        display: -ms-flexbox;
        -webkit-box-align: center;
        -ms-flex-align: center;
        -webkit-justify-content: center;
        -ms-flex-pack: center;
    }
}
body.mini-sidebar .sidebar .sidebar-menu > ul > li.submenu-open ul li a {
    @apply flex items-center justify-center font-medium text-[15px] text-[#67748E] relative w-full;
    display: -webkit-box;
    display: -ms-flexbox;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-justify-content: center;
    -ms-flex-pack: center;
}
body.mini-sidebar .sidebar .sidebar-menu > ul > li ul > li.active a {
    @apply justify-center;
}
body.mini-sidebar .sidebar .sidebar-menu > ul li a {
    @apply justify-center;
}
body.mini-sidebar.expand-menu
    .sidebar
    .sidebar-menu
    > ul
    > li.submenu-open
    ul
    li
    a {
    @apply pl-[18px];
    justify-content: left;
    -webkit-justify-content: left;
    -ms-flex-pack: left;
}
body.mini-sidebar.expand-menu
    .sidebar
    .sidebar-menu
    > ul
    > li
    ul
    > li.active
    a {
    justify-content: start;
}
body.mini-sidebar.expand-menu .sidebar .user-names {
    @apply block;
}
body.mini-sidebar.expand-menu .sidebar .sidebar-menu > ul li a {
    justify-content: start;
}
.sidebarrightmenu .sidebar-right {
    @apply block;
}
.slide-nav .sidebar {
    @apply ml-0;
}
.sidebar-overlay {
    @apply hidden h-full w-full z-[99] bg-[rgba(0,0,0,0.6)] fixed left-0 top-[60px];
}
.sidebar-overlay.opened {
    @apply block;
}
.mini-sidebar .active.subdrop ~ ul {
