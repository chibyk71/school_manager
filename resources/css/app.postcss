.sidebar {
    @apply flex bg-[var(--app-header-bg)] z-[1001] fixed transition-all duration-[0.5s] ease-[ease] w-[252px] mt-0 border-r-[--theme-bg-subtle] border-r border-solid left-0 top-14 bottom-0;

    .sidebar-inner {
        @apply w-full;
    }

    .sidebar-menu {
        @apply p-[15px];

        & > ul {
            li .submenu > ul li a {
                @apply text-sm relative pl-10 before:content-[""] before:w-[5px] before:h-[5px] before:absolute before:-translate-y-2/4 before:rounded-[50%] before:left-7 before:top-2/4 before:bg-primary-200;
            }

            & > li {
                @apply mb-6;

                &:last-child ul {
                    @apply mb-0;
                }
                &.active a {
                    @apply text-[--p-text-color] rounded-[5px];
                }
                &.active svg {
                    @apply text-[--p-primary-300];
                }
                svg {
                    @apply min-w-[16px] w-4 h-4 mr-2.5;
                }
                &> a {
                    @apply flex items-center relative text-primary px-[15px] py-2.5;
                }
                &.submenu ul li a {
                    @apply font-medium text-sm relative block pl-[30px] pr-2 py-2;
                }
                ul {
                    /* @apply block mb-[15px]; */
                    li {
                        @apply overflow-hidden mb-[5px] rounded-[5px];
                        a {
                            @apply flex items-center font-normal text-sm dark:text-primary text-color relative w-full p-2 dark:hover:text-[--p-primary-hover-color];
                            &.active span {
                                @apply text-primary-contrast;
                            }
                        }
                    }
                }
            }
        }

    }
}
