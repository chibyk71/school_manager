@import "primeicons/primeicons.css";

/* Use @import */
@import "tailwindcss/base";
@import "tailwindcss/components";
@import "../../public/assets/primevue/tailwind.css";
@import "tailwindcss/utilities";

/* Light */
:root {
    --p-primary-50: #ecfdf5;
    --p-primary-100: #d1fae5;
    --p-primary-200: #a7f3d0;
    --p-primary-300: #6ee7b7;
    --p-primary-400: #34d399;
    --p-primary-500: #10b981;
    --p-primary-600: #059669;
    --p-primary-700: #047857;
    --p-primary-800: #065f46;
    --p-primary-900: #064e3b;
    --p-primary-950: #022c22;
    --p-surface-0: #ffffff;
    --p-surface-50: #fafafa;
    --p-surface-100: #f4f4f5;
    --p-surface-200: #e4e4e7;
    --p-surface-300: #d4d4d8;
    --p-surface-400: #a1a1aa;
    --p-surface-500: #71717a;
    --p-surface-600: #52525b;
    --p-surface-700: #3f3f46;
    --p-surface-800: #27272a;
    --p-surface-900: #18181b;
    --p-surface-950: #09090b;
    --p-content-border-radius: 6px;
    --p-primary-color: var(--p-primary-500);
    --p-primary-contrast-color: var(--p-surface-0);
    --p-primary-hover-color: var(--p-primary-600);
    --p-primary-active-color: var(--p-primary-700);
    --p-content-border-color: var(--p-surface-200);
    --p-content-hover-background: var(--p-surface-100);
    --p-content-hover-color: var(--p-surface-800);
    --p-highlight-background: var(--p-primary-50);
    --p-highlight-color: var(--p-primary-700);
    --p-highlight-focus-background: var(--p-primary-100);
    --p-highlight-focus-color: var(--p-primary-800);
    --p-text-color: var(--p-surface-700);
    --p-text-hover-color: var(--p-surface-800);
    --p-text-muted-color: var(--p-surface-500);
    --p-text-hover-muted-color: var(--p-surface-600);
    --body-bg-gradient: linear-gradient(
        180deg,
        rgba(245, 254, 253, 0.9) 0%,
        rgba(255, 255, 255, 0.99) 100%
    );
    --body-bg-image: url(/assets/img/bg/patternlight.png);
    --body-bg-image-size: 4.6875rem;
    --body-bg-cover: url(/hud-vue/assets/cover-dhlOIT5m.jpg);
    --body-font-weight: 400;
    --body-letter-spacing: 0.046875rem;
    --app-header-bg: #ffffff;
    --app-header-brand-img: url(data:image/png;base64,);
    --app-header-link-color: #000000;
    --app-header-link-color-rgb: 0, 0, 0;
    --app-header-link-hover-color: #000000;
    --app-header-scroll-box-shadow: ;
    --app-sidebar-menu-header-color: rgba(var(--inverse-rgb), 0.3);
    --app-sidebar-menu-link-color: rgba(var(--inverse-rgb), 0.5);
    --app-sidebar-menu-link-hover-color: #000000;
    --app-sidebar-menu-link-active-color: #000000;
    --app-sidebar-mobile-bg: rgba(255, 255, 255, 0.98);
    --app-top-nav-bg: #ffffff;
    --app-top-nav-menu-link-color: rgba(var(--inverse-rgb), 0.5);
    --app-top-nav-menu-link-hover-color: #000000;
    --app-top-nav-menu-link-active-color: #000000;
    --app-top-nav-menu-submenu-bg: #ffffff;
    --app-theme-panel-bg: rgba(255, 255, 255, 0.95);
    --theme-bg-subtle: #d3ebe4;
}

/*
 * Dark Mode
 * Defaults to system, change the selector to match the darkMode in tailwind.config.
 * For example;
 * darkMode: ['selector', '[class*="app-dark"]']
 * should be;
 * :root[class="app-dark"] {
*/
:root[class="dark"] {
    --p-primary-color: var(--p-primary-400);
    --p-primary-contrast-color: var(--p-surface-900);
    --p-primary-hover-color: var(--p-primary-300);
    --p-primary-active-color: var(--p-primary-200);
    --p-content-border-color: var(--p-surface-700);
    --p-content-hover-background: var(--p-surface-800);
    --p-content-hover-color: var(--p-surface-0);
    --p-highlight-background: color-mix(
        in srgb,
        var(--p-primary-400),
        transparent 84%
    );
    --p-highlight-color: rgba(255, 255, 255, 0.87);
    --p-highlight-focus-background: color-mix(
        in srgb,
        var(--p-primary-400),
        transparent 76%
    );
    --p-highlight-focus-color: rgba(255, 255, 255, 0.87);
    --p-text-color: var(--p-surface-0);
    --p-text-hover-color: var(--p-surface-0);
    --p-text-muted-color: var(--p-surface-400);
    --p-text-hover-muted-color: var(--p-surface-300);

    --body-bg-gradient: linear-gradient(
        180deg,
        rgba(50, 70, 80, 0.9) 0%,
        rgb(13, 16, 27) 100%
    );
    --body-bg-image: url(data:image/png;base64,);
    --body-font-weight: 300;
    --body-letter-spacing: 0.046875rem;
    --component-bg: #28292b;
    --component-bg-rgb: 40, 41, 43;
    --component-color: rgba(255, 255, 255, 0.75);
    --component-color-rgb: 255, 255, 255;
    --app-header-bg: rgba(29, 40, 53, 0.95);
    --app-header-link-color: #ffffff;
    --app-header-link-color-rgb: 255, 255, 255;
    --app-header-link-hover-color: rgba(255, 255, 255, 0.5);
    --app-header-scroll-box-shadow: ;
    --app-sidebar-menu-header-color: rgba(var(--inverse-rgb), 0.3);
    --app-sidebar-menu-link-color: rgba(var(--inverse-rgb), 0.5);
    --app-sidebar-menu-link-hover-color: #ffffff;
    --app-sidebar-menu-link-active-color: #ffffff;
    --app-sidebar-mobile-bg: rgba(44, 56, 70, 0.98);
    --app-top-nav-bg: rgba(29, 40, 53, 0.95);
    --app-top-nav-menu-link-color: rgba(var(--inverse-rgb), 0.5);
    --app-top-nav-menu-link-hover-color: #ffffff;
    --app-top-nav-menu-link-active-color: #ffffff;
    --app-top-nav-menu-submenu-bg: rgba(29, 40, 53, 0.95);
    --app-theme-panel-bg: rgba(68, 85, 94, 0.95);
    --theme-bg-subtle: #071f18;
}

html:before {
    content: "";
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    height: 100%;
    z-index: -5;
    background: var(--body-bg-gradient);
}

body:before {
    content: "";
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    height: 100%;
    background-color: transparent;
    background-image: var(--body-bg-image);
    background-size: var(--body-bg-image-size);
    background-repeat: repeat;
    background-attachment: initial;
    z-index: -5;
}

.p-menubar.app-header {
    @apply h-14 bg-[var(--app-header-bg)] transition-all duration-150 ease-in-out border-0 rounded-t-md;
    & .brand {
        height: 100%;
        width: 18.875rem;
        padding: 0 1rem;
        display: flex;
        align-items: center;

        & .brand-logo {
            text-decoration: none;
            color: var(--app-header-link-color);
            font-size: 0.91875rem;
            letter-spacing: 2px;
            display: flex;
            align-items: center;

            & .brand-img {
                width: 1.625rem;
                height: 1.625rem;
                background: var(--app-header-brand-img);
                background-size: cover;
                background-position: center;
                background-repeat: no-repeat;
                display: flex;
                align-items: center;
                justify-content: center;
                margin-right: 1rem;

                & .brand-img-text {
                    display: block;
                    letter-spacing: initial;
                    font-weight: 600;
                    font-size: 1.05rem;
                }
            }
        }
    }
}
